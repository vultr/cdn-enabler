{
    "name": "vultr/cdn-enabler",
    "description": "A content delivery network (CDN) integration plugin for WordPress that rewrites URLs, like for CSS, JavaScript, and images, to be served by a CDN.",
    "type": "wordpress-plugin",
    "homepage": "https://github.com/vultr/cdn-enabler",
    "license": "GPL-2.0-or-later",
    "support": {
        "issues": "https://github.com/vultr/cdn-enabler/issues",
        "source": "https://github.com/vultr/cdn-enabler"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
